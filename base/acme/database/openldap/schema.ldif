olcAttributeTypes: ( 2.16.840.1.113730.5.2.1.4 NAME 'acmeStatus'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  EQUALITY caseIgnoreMatch
  SINGLE-VALUE )
